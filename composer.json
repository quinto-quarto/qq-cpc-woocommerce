{
    "name": "quintoquarto/qq-cpc-woocommerce",
    "description": "Check and update WooCommerce order statuses using CPC API",
    "version": "0.2.0",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Quinto Quarto",
            "email": "info@quintoquarto.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "QQCPC\\": "inc/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "brain/monkey": "^2.6",
        "mockery/mockery": "^1.4"
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --configuration=phpunit-unit.xml",
        "test:integration": "phpunit --testsuite=integration",
        "test:unit-only": "phpunit --testsuite=unit --no-coverage",
        "install-wp-tests": "bash bin/install-wp-tests.sh wordpress_test root '' localhost latest"
    },
    "minimum-stability": "dev"
}
